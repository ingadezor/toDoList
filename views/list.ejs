
    <%- include('partials/header') -%>
        
        <div class="box" id="heading">
            <h1><%= title %></h1>
        </div>
        
        <!-- LIST-->
        <div class="box">
                <%   for(let i=0; i<list.length; i++){   %>
                    <div class="item"> 
                        <input type="checkbox">
                        <p> <%= list[i] %> </p>
                    </div>
                <%  }  %>
                
                
                 <!--Posting new item entered bya user to server -->
                <form class="item" action="/" method="POST">
                    <input type="text" name="listItem" value="" placeholder="New Item" autocomplete="off">
                    <button type="submit" name='button' value=<%= title %>>+</button>
                </form>
        </div>
        

        <%- include('partials/footer') -%>

       
